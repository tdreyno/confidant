{
  "name": "@tdreyno/confidant",
  "version": "0.0.0",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "repository": "https://github.com/tdreyno/confidant.git",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "build": "pika build",
    "test": "jest --runInBand --forceExit",
    "version": "yarn run build",
    "commit": "git-cz",
    "coverage": "jest --runInBand --forceExit --coverage --coverageDirectory '../coverage'",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ]
    ]
  },
  "dependencies": {
    "async-retry": "^1.3.1",
    "aws-sdk": "^2.943.0",
    "date-fns": "^2.23.0",
    "jsonwebtoken": "^8.5.1",
    "launchdarkly-node-server-sdk": "^6.2.0",
    "ms": "^2.1.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@commitlint/prompt": "^12.1.4",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "^0.9.2",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@types/async-retry": "^1.4.3",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/ms": "^0.7.31",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "msw": "^0.32.0",
    "node-fetch": "^2.6.1",
    "prettier": "^2.3.2",
    "semantic-release": "^17.4.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint -c .eslintrc.js --ext .ts --ext .tsx src --fix",
      "prettier --write"
    ],
    "*.{css,md}": [
      "prettier --write"
    ]
  },
  "volta": {
    "node": "14.11.0",
    "yarn": "1.22.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
